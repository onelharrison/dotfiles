export PATH="$HOME/.local/bin:$PATH"

export PROMPT="%B%F{240}%2~%f%b "$'\n'"%(?.%F{123}λ.%F{red}λ)%f "

. $HOME/github.com/onelharrison/dotfiles/conf/zsh/git.zsh

autoload -Uz vcs_info
precmd_vcs_info() { vcs_info }
precmd_functions+=( precmd_vcs_info )
setopt prompt_subst
RPROMPT=\$vcs_info_msg_0_
zstyle ':vcs_info:git:*' formats '%F{240}(%b) %r%f'
zstyle ':vcs_info:*' enable git

. $HOME/github.com/onelharrison/dotfiles/conf/zsh/rails.zsh

. $HOME/github.com/onelharrison/dotfiles/conf/zsh/bundler.zsh

. $HOME/github.com/onelharrison/dotfiles/conf/zsh/xclip.zsh

. $HOME/github.com/onelharrison/dotfiles/conf/zsh/zshrc_extended

export GOPATH="$HOME/go"; export GOROOT="$HOME/.go"; export PATH="$GOPATH/bin:$PATH"; # g-install: do NOT edit, see https://github.com/stefanmaric/g
alias ggovm="$GOPATH/bin/g"; # g-install: do NOT edit, see https://github.com/stefanmaric/g
